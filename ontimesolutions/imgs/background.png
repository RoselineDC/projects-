        <script>
            $(document).ready(function () {
                $("#hid-inp").val(1);
                var slideLen = $(".home-banner").length;

                for (i = 1; i <= slideLen; i++) {
                    $(".home-slide-radio").append("<span onClick = 'radioClick(" + i + ")' class = 'radio-slide' id = 'radio-slide-" + i + "'></span>");
                }

                $("#radio-slide-1").addClass("active-radio");

                // Call nextClick every 5 seconds (5000 milliseconds)
                setInterval(nextClick, 5000);
            });

            function radioClick(i) {
                $(".radio-slide").removeClass("active-radio");
                $("#radio-slide-" + i).addClass("active-radio");
                $(".home-banner").fadeOut();
                setTimeout(function () {
                    $("#home-banner-" + i).fadeIn();
                }, 100);
                $("#hid-inp").val(i);
            }

            function nextClick() {
                var counter = parseInt($("#hid-inp").val());
                var slideLen = $(".home-banner").length;

                $("#home-banner-" + (counter)).fadeOut();

                if (counter < slideLen) {
                    counter += 1;
                } else {
                    counter = 1;
                }
                radioClick(counter);
                setTimeout(function () {
                    $("#home-banner-" + counter).fadeIn();
                }, 100);

                $("#hid-inp").val(counter);
            }

            function prevClick() {
                var counter = parseInt($("#hid-inp").val());
                var slideLen = $(".home-banner").length;

                $("#home-banner-" + (counter)).fadeOut();

                if (counter > 1) {
                    counter -= 1;
                } else {
                    counter = slideLen;
                }

                radioClick(counter);
                setTimeout(function () {
                    $("#home-banner-" + counter).fadeIn();
                }, 100);

                $("#hid-inp").val(counter);
            }
        </script>






